{
  "$schema": "https://vim-fall.github.io/jsonschema/v1/extension.schema.json",
  "$version": "1",
  "action": {
    "debug": {
      "uri": "builtin:debug"
    },
    "open": {
      "uri": "builtin:open",
      "variants": {
        "split": {
          "opener": "split"
        },
        "vsplit": {
          "opener": "vsplit"
        },
        "tabedit": {
          "opener": "tabedit"
        },
        "edit-split": {
          "splitter": "split"
        },
        "edit-vsplit": {
          "splitter": "vsplit"
        },
        "edit-tabedit": {
          "splitter": "tabedit"
        }
      }
    }
  },
  "previewer": {
    "path": {
      "uri": "builtin:path"
    }
  },
  "processor": {
    "substring_filter": {
      "uri": "builtin:substring_filter"
    },
    "lexical_sorter": {
      "uri": "builtin:lexical_sorter",
      "variants": {
        "reverse": {
          "reverse": true
        }
      }
    }
  },
  "renderer": {},
  "source": {
    "list": {
      "uri": "builtin:list"
    },
    "file": {
      "uri": "builtin:file",
      "options": {
        "excludes": [
          "/.git/",
          "/node_modules/",
          "/target/",
          "/.DS_Store",
          "/Desktop.ini",
          "/Thumbs.db"
        ]
      }
    },
    "line": {
      "uri": "builtin:line"
    }
  }
}
