{
  "$schema": "https://vim-fall.github.io/jsonschema/v1/extension-config.schema.json",
  "action": {
    "open": {
      "uri": "builtin:action/open",
      "variants": {
        "split": {
          "opener": "split"
        },
        "vsplit": {
          "opener": "vsplit"
        },
        "tabedit": {
          "opener": "tabedit"
        },
        "edit-split": {
          "splitter": "split"
        },
        "edit-vsplit": {
          "splitter": "vsplit"
        },
        "edit-tabedit": {
          "splitter": "tabedit"
        }
      }
    }
  },
  "previewer": {
    "path": {
      "uri": "builtin:previewer/path"
    }
  },
  "processor": {
    "substring_filter": {
      "uri": "builtin:processor/substring_filter"
    },
    "lexical_sorter": {
      "uri": "builtin:processor/lexical_sorter",
      "variants": {
        "reverse": {
          "reverse": true
        }
      }
    }
  },
  "source": {
    "file": {
      "uri": "builtin:source/file",
      "options": {
        "excludes": [
          "/.git/",
          "/node_modules/",
          "/target/",
          "/.DS_Store",
          "/Desktop.ini",
          "/Thumbs.db"
        ]
      },
      "variants": {
        "file:all": {
          "excludes": []
        }
      }
    },
    "line": {
      "uri": "builtin:source/line"
    }
  }
}
