{
  "$schema": "https://vim-fall.github.io/jsonschema/v1alpha/extension-config.schema.json",
  "action": {
    "open": {
      "url": "fallbuiltin://action/open.ts",
      "variants": {
        "split": {
          "opener": "split"
        },
        "vsplit": {
          "opener": "vsplit"
        },
        "tabedit": {
          "opener": "tabedit"
        },
        "edit-split": {
          "splitter": "split"
        },
        "edit-vsplit": {
          "splitter": "vsplit"
        },
        "edit-tabedit": {
          "splitter": "tabedit"
        }
      }
    },
    "quickfix": {
      "url": "https://github.com/vim-fall/package-common/blob/main/action/quickfix.ts",
      "variants": {
        "add": {
          "action": "a"
        },
        "replace": {
          "action": "r"
        },
        "processed": {
          "target": "selected-or-processed"
        },
        "processed:add": {
          "target": "selected-or-processed",
          "action": "a"
        },
        "processed:replace": {
          "target": "selected-or-processed",
          "action": "r"
        }
      }
    }
  },
  "processor": {
    "lexical_sorter": {
      "url": "https://github.com/vim-fall/package-common/blob/main/processor/lexical_sorter.ts",
      "variants": {
        "reverse": {
          "reverse": true
        }
      }
    }
  },
  "source": {
    "file": {
      "url": "fallbuiltin://source/file.ts",
      "options": {
        "excludes": [
          "/.git/",
          "/node_modules/",
          "/target/",
          "/.DS_Store",
          "/Desktop.ini",
          "/Thumbs.db"
        ]
      },
      "variants": {
        "file:all": {
          "excludes": []
        }
      }
    },
    "line": {
      "url": "https://github.com/vim-fall/package-common/blob/main/source/line.ts"
    }
  }
}
